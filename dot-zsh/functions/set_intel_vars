## path variables
_CLASSPATH=${_I_MPI_ROOT}/lib/mpi.jar:${_DALROOT}/lib/onedal.jar:
_CMAKE_PREFIX_PATH=${_ONEAPI_ROOT}/vpl/2022.0.0:${_TBBROOT}:${_DALROOT}:${_CMPLR_ROOT}/linux/IntelDPCPP:
