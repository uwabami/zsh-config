_CPLUS_INCLUDE_PATH=${_CLCK_ROOT}/include
_GDB_INFO=${_ONEAPI_ROOT}/debugger/2021.5.0/documentation/info/
_INFOPATH=${_ONEAPI_ROOT}/debugger/2021.5.0/gdb/${_ARCH}/lib
_LD_LIBRARY_PATH=${_ONEAPI_ROOT}/vpl/2022.0.0/lib:${_TBBROOT}/lib/${_ARCH}/gcc4.8:${_I_MPI_ROOT}/libfabric/lib:${_I_MPI_ROOT}/lib/release:${_I_MPI_ROOT}/lib:${_MKLROOT}/lib/${_ARCH}:${_VT_ROOT}/slib:${_IPPCRYPTOROOT}/lib/${_ARCH}:${_IPPROOT}/lib/${_ARCH}:${_DNNLROOT}/lib:${_ONEAPI_ROOT}/debugger/2021.5.0/gdb/${_ARCH}/lib:${_ONEAPI_ROOT}/debugger/2021.5.0/libipt/${_ARCH}/lib:${_ONEAPI_ROOT}/debugger/2021.5.0/dep/lib:${_DALROOT}/lib/${_ARCH}:${_CMPLR_ROOT}/linux/lib:${_CMPLR_ROOT}/linux/lib/x64:${_INTELFPGAOCLSDKROOT}/host/linux64/lib:${_CMPLR_ROOT}/linux/compiler/lib/${_ARCH}_lin:${_CCL_ROOT}/lib/${_CCL_CONFIGURATION}
_LIBRARY_PATH=${_ONEAPI_ROOT}/vpl/2022.0.0/lib:${_TBBROOT}/lib/${_ARCH}/gcc4.8:${_I_MPI_ROOT}/libfabric/lib:${_I_MPI_ROOT}/lib/release:${_I_MPI_ROOT}/lib:${_MKLROOT}/lib/${_ARCH}:${_IPPCRYPTOROOT}/lib/${_ARCH}:${_IPPROOT}/lib/${_ARCH}:${_DNNLROOT}/lib:${_DALROOT}/lib/${_ARCH}:${_CMPLR_ROOT}/linux/compiler/lib/${_ARCH}_lin:${_CMPLR_ROOT}/linux/lib:${_CLCK_ROOT}/lib/${_ARCH}:${_CCL_ROOT}/lib/${_CCL_CONFIGURATION}
_MANPATH=${_I_MPI_ROOT}/man:${_VT_ROOT}/man:${_ONEAPI_ROOT}/debugger/2021.5.0/documentation/man:${_CMPLR_ROOT}/documentation/en/man/common:${_CLCK_ROOT}/man:
