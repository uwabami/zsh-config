_PATH=${_ONEAPI_ROOT}/vtune/2022.1.0/bin64:${_ONEAPI_ROOT}/vpl/2022.0.0/bin:${_I_MPI_ROOT}/libfabric/bin:${_I_MPI_ROOT}/bin:${_MKLROOT}/bin/${_ARCH}:${_VT_ROOT}/bin:${_ONEAPI_ROOT}/intelpython/latest/bin:${_ONEAPI_ROOT}/intelpython/latest/condabin:${_ONEAPI_ROOT}/inspector/2022.0.0/bin64:${_DPL_ROOT}/bin:${_ONEAPI_ROOT}/dev-utilities/2021.5.2/bin:${_ONEAPI_ROOT}/debugger/2021.5.0/gdb/${_ARCH}/bin:${_INTELFPGAOCLSDKROOT}/bin:${_CMPLR_ROOT}/linux/bin/${_ARCH}:${_CMPLR_ROOT}/linux/bin:${_CLCK_ROOT}/bin/${_ARCH}:${_ADVISOR_2022_DIR}/bin64:
_PKG_CONFIG_PATH=${_ONEAPI_ROOT}/vtune/2022.1.0/include/pkgconfig/lib64:${_ONEAPI_ROOT}/vpl/2022.0.0/lib/pkgconfig:${_TBBROOT}/lib/pkgconfig:${_I_MPI_ROOT}/lib/pkgconfig:${_MKLROOT}/lib/pkgconfig:${_IPPCRYPTOROOT}/lib/pkgconfig:${_ONEAPI_ROOT}/inspector/2022.0.0/include/pkgconfig/lib64:${_ONEAPI_ROOT}/dpl/2021.6.0/lib/pkgconfig:${_DNNLROOT}/../lib/pkgconfig:${_DALROOT}/lib/pkgconfig:${_CMPLR_ROOT}/lib/pkgconfig:${_CCL_ROOT}/lib/pkgconfig:${_ADVISOR_2022_DIR}/include/pkgconfig/lib64:

# other
_APM=${_ADVISOR_2022_DIR}/perfmodels
