#! /bin/zsh
#
# common
_ONEAPI_ROOT=/opt/intel/oneapi
_ARCH=intel64
_CCL_CONFIGURATION=cpu_gpu_dpcpp
_FPGA_VARS_ARGS=ia64
# components root dir, variables
